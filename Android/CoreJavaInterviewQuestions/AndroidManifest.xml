<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sample.code.corejavainterviewquestion"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/java_icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".questions.StringQuestions"></activity>
        <activity android:name=".questions.ExceptionQuestions"></activity>
        <activity android:name=".questions.ThreadQuestions"></activity>
        <activity android:name=".questions.BasicQuestions"></activity>
        <activity android:name=".questions.CollectionQuestions"></activity>
    </application>

</manifest>
