<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<groupId>com.odcgroup.dost</groupId>
	<artifactId>dost-dependencies</artifactId>
	<version>1.0-SNAPSHOT</version>
	<packaging>pom</packaging>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-dependency-plugin</artifactId>
				<configuration>
					<artifactItems>
						<artifactItem>
							<groupId>avalon-framework</groupId>
							<artifactId>avalon-framework</artifactId>
							<version>4.1.4</version>
							<type>jar</type>
							<overWrite>true</overWrite>
							<outputDirectory>.</outputDirectory>
							<destFileName>avalon-framework.jar</destFileName>
						</artifactItem>
						<artifactItem>
							<groupId>batik</groupId>
							<artifactId>batik</artifactId>
							<version>1.5</version>
							<type>jar</type>
							<overWrite>true</overWrite>
							<outputDirectory>.</outputDirectory>
							<destFileName>batik.jar</destFileName>
						</artifactItem>
						<artifactItem>
							<groupId>net.sourceforge.dita-ot</groupId>
							<artifactId>dost</artifactId>
							<version>1.4</version>
							<type>jar</type>
							<overWrite>true</overWrite>
							<outputDirectory>.</outputDirectory>
							<destFileName>dost.jar</destFileName>
						</artifactItem>
						<artifactItem>
							<groupId>fop</groupId>
							<artifactId>fop</artifactId>
							<version>0.20.5</version>
							<type>jar</type>
							<overWrite>true</overWrite>
							<outputDirectory>.</outputDirectory>
							<destFileName>fop.jar</destFileName>
						</artifactItem>
						<artifactItem>
							<groupId>com.ibm.icu</groupId>
							<artifactId>icu4j</artifactId>
							<version>53.1</version>
							<type>jar</type>
							<overWrite>true</overWrite>
							<outputDirectory>.</outputDirectory>
							<destFileName>icu4j.jar</destFileName>
						</artifactItem>
						<artifactItem>
							<groupId>xml-resolver</groupId>
							<artifactId>xml-resolver</artifactId>
							<version>1.2</version>
							<type>jar</type>
							<overWrite>true</overWrite>
							<outputDirectory>.</outputDirectory>
							<destFileName>resolver.jar</destFileName>
						</artifactItem>
						<artifactItem>
							<groupId>xalan</groupId>
							<artifactId>xalan</artifactId>
							<version>2.7.1.jbossorg-2</version>
							<type>jar</type>
							<overWrite>true</overWrite>
							<outputDirectory>.</outputDirectory>
							<destFileName>xalan.jar</destFileName>
						</artifactItem>
						<artifactItem>
							<groupId>xerces</groupId>
							<artifactId>xercesImpl</artifactId>
							<version>2.11.0</version>
							<type>jar</type>
							<overWrite>true</overWrite>
							<outputDirectory>.</outputDirectory>
							<destFileName>xercesImpl.jar</destFileName>
						</artifactItem>
						<artifactItem>
							<groupId>xml-apis</groupId>
							<artifactId>xml-apis</artifactId>
							<version>2.0.2</version>
							<type>jar</type>
							<overWrite>true</overWrite>
							<outputDirectory>.</outputDirectory>
							<destFileName>xml-apis.jar</destFileName>
						</artifactItem>
					</artifactItems>

				</configuration>
			</plugin>
		</plugins>
	</build>
</project>
