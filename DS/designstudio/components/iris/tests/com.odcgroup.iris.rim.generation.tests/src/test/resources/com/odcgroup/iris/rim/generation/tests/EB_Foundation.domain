Classes {
	/* From the Single Customer View, the Account opening process can be initiated. The process starts from selecting of account type. After entering the account details an Account opening letter is generated. Request for Cheque book and Debit card can be given. If the product type is "Current Account with Overdraft", a limit is created and attached with the account at the backend. */
	AC_ACC_OPENING
		@t24:i (t24Name=AC.ACC.OPENING, level=F, type=D, allowedFunctions="A C D E I L Q S", localRefAllowed=false)
	{
		DEAL_NO -> EB_Foundation:AC_ACC_OPENING PK required
			@i18n:Label (en="Deal Number", es="Número de Contrato")
			@t24:i (align=L, inputBehaviour=I, maxLength=50, sysNumber=0.0, core=true, businessType=A, t24Name=DEAL.NO)
		/* The field "Category" holds dropdown values .Type of account can be selected from the drop down. */
		CATEGORY -> ST_Config:CATEGORY
			@i18n:Label (en=Category, fr=Produit, es="Categoría")
			@i18n:Tooltip (en="Specifies the Category", fr="Code du produit")
			@t24:i (align=L, inputBehaviour=I, maxLength=5, sysNumber=1.0, core=true, businessType=A, t24Name=CATEGORY)
		/* This field displays Customer Number which can be inputted by the user or chosen from the dropdown enquiry. */
		CUSTOMER -> ST_Customer:CUSTOMER
			@i18n:Label (en=Customer, fr=Client, es=Cliente)
			@i18n:Tooltip (en="Customer Liability", fr="Code du client", es="Pasivo de Cliente")
			@t24:i (align=R, inputBehaviour=I, maxLength=8, sysNumber=2.0, core=true, businessType=CUS, t24Name=CUSTOMER)
		/* This field displays Currency which can be inputted by the user or chosen from the dropdown enquiry. */
		CURRENCY -> FIN_CurrencyConfig:CURRENCY
			@i18n:Label (en=Ccy, fr=Devise, es=Moneda)
			@i18n:Tooltip (fr="Code devise ISO")
			@t24:i (align=L, inputBehaviour=I, maxLength=5, sysNumber=3.0, core=true, businessType=CCY, t24Name=CURRENCY)
		OVERRIDE -> byValue EB_Foundation:AC_ACC_OPENING__OVERRIDE *
		RECORD_STATUS : T24BusinessTypes:STAT
			@i18n:Label (en="Record Status", fr=Statut, es=Estado)
			@i18n:Tooltip (fr="Statut de l\'enregistrement")
			@t24:i (align=L, inputBehaviour=N, maxLength=4, sysNumber=5.0, core=true, t24Name=RECORD.STATUS)
		CURR_NO : mml:Integer
			@i18n:Label (en="Curr No", fr="N° version", es="No. Actual")
			@i18n:Tooltip (fr="Numéro de version")
			@t24:i (align=R, inputBehaviour=N, maxLength=4, sysNumber=6.0, core=true, t24Name=CURR.NO)
		INPUTTER -> byValue EB_Foundation:AC_ACC_OPENING__INPUTTER *
		DATE_TIME -> byValue EB_Foundation:AC_ACC_OPENING__DATE_TIME *
		AUTHORISER : T24BusinessTypes:A
			@i18n:Label (en=Authoriser, fr="Validé par", es=Autorizador)
			@i18n:Tooltip (en="Indicates the User who Authorises the record", fr="Utilisateur de validation", es="El usuario que autorizó el registro")
			@t24:i (align=L, inputBehaviour=N, maxLength=40, sysNumber=9.0, core=true, t24Name=AUTHORISER)
		DEPT_CODE -> ST_Config:DEPT_ACCT_OFFICER
			@i18n:Label (en="Dept Code", fr=Service, es=Departamento)
			@i18n:Tooltip (fr="Code du service")
			@t24:i (align=R, inputBehaviour=N, maxLength=4, sysNumber=11.0, core=true, t24Name=DEPT.CODE)
		AUDITOR_CODE : T24BusinessTypes:A
			@i18n:Label (en="Auditor Code", fr="Audité par", es="Código de Auditor")
			@i18n:Tooltip (en="Auditor Code", fr="Utilisateur d\'audit", es="Nombre del auditor")
			@t24:i (align=L, inputBehaviour=E, maxLength=16, sysNumber=12.0, core=true, t24Name=AUDITOR.CODE)
		AUDIT_DATE_TIME : T24BusinessTypes:RELTIME
			@i18n:Label (en="Audit Date Time", fr="Audité le", es="Fecha/Hora de Audit.")
			@i18n:Tooltip (en="Date and time when record was audited", fr="Date et heure d\'audit", es="Fecha y Hora de Auditoría ")
			@t24:i (align=L, inputBehaviour=E, maxLength=15, sysNumber=13.0, core=true, t24Name=AUDIT.DATE.TIME, mask="RDD DD  DD ##:##")
	}
	AC_ACC_OPENING__OVERRIDE
	{
		OVERRIDE : T24BusinessTypes:A
			@i18n:Label (en=Override, fr=Alerte, es=Alerta)
			@i18n:Tooltip (en="Overrides generated by this transaction", fr="Message d\'alerte", es="Alertas generadas por esta transacción")
			@t24:i (align=L, inputBehaviour=N, maxLength=35, sysNumber=4.0, core=true, t24Name=OVERRIDE)
	}
	AC_ACC_OPENING__INPUTTER
	{
		INPUTTER : T24BusinessTypes:A
			@i18n:Label (en=Inputter, fr="Saisi par", es=Digitador)
			@i18n:Tooltip (en="The user who last updated the record", fr="Utilisateur de saisie", es="El usuario que actualizó de último el registro")
			@t24:i (align=L, inputBehaviour=N, maxLength=40, sysNumber=7.0, core=true, t24Name=INPUTTER)
	}
	AC_ACC_OPENING__DATE_TIME
	{
		DATE_TIME : T24BusinessTypes:RELTIME
			@i18n:Label (en="Date time", fr="Saisi le", es="Fecha Hora")
			@i18n:Tooltip (en="Date and time when record was input or authorised", fr="Date et heure de saisie", es="Fecha/hora cuando registro fue entrado/autorizado")
			@t24:i (align=L, inputBehaviour=N, maxLength=15, sysNumber=8.0, core=true, t24Name=DATE.TIME, mask="RDD DD  DD ##:##")
	}	
