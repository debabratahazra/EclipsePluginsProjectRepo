<?xml version="1.0" encoding="UTF-8"?>
<workflow:workflow xmlns:workflow="http://www.odcgroup.com/workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.odcgroup.com/workflow workflow.xsd">
	<!-- process p5 -->	
	<workflow:process name="p5" displayName="process p5" description="test process">	
		<workflow:version-comment></workflow:version-comment>
		<workflow:activity name="a" displayName="activity" description="an activity" initial="true" group="Pool1" >
			<workflow:application>	
					<workflow:url>inc</workflow:url>
			</workflow:application>
			
			<workflow:assignees>
				<workflow:assignee-ref name="orga:assigneeA"/>
			</workflow:assignees>
			<workflow:next activity="b" />
			<workflow:next activity="c" />
			<workflow:selector>
				<workflow:script language="jython"><![CDATA[#test
if output == 2: 
  selection.add("b") 
elif output == 3: 
  selection.add("c") 
elif output==4: 
  selection.add("b") 
  selection.add("c") 
else: 
  print "wrong output"
]]></workflow:script>
			</workflow:selector>			
		</workflow:activity>
		<workflow:activity name="b" displayName="activity" description="an activity" personal="true" group="Pool2" >
			<workflow:application>	
					<workflow:url>inc</workflow:url>
			</workflow:application>
			
			<workflow:assignees>
				<workflow:assignee-ref name="orga:assigneeB"/>
			</workflow:assignees>
			<workflow:next activity="d" />
		</workflow:activity>
		<workflow:activity name="c" displayName="activity" description="an activity" personal="true" group="Pool3" >
			<workflow:application>	
					<workflow:url>inc</workflow:url>
			</workflow:application>
			
			<workflow:assignees>
				<workflow:assignee-ref name="orga:assigneeC"/>
			</workflow:assignees>
			<workflow:next activity="d" />
		</workflow:activity>
		<workflow:activity name="d" displayName="activity" description="an activity" personal="true" group="Pool3" >
			<workflow:application>	
					<workflow:url>inc</workflow:url>
			</workflow:application>
			
			<workflow:assignees>
				<workflow:assignee-ref name="orga:assigneeC"/>
			</workflow:assignees>
			<workflow:pre-selector>				
				<workflow:condition type="AND">
					<workflow:completed activity="b"/>
					<workflow:completed activity="c"/>
				</workflow:condition>
			</workflow:pre-selector>
		</workflow:activity>
	</workflow:process>	
</workflow:workflow>
