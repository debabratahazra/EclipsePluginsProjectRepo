<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<wls:weblogic-ejb-jar xmlns:wls="http://xmlns.oracle.com/weblogic/weblogic-ejb-jar" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd http://xmlns.oracle.com/weblogic/weblogic-ejb-jar http://xmlns.oracle.com/weblogic/weblogic-ejb-jar/1.1/weblogic-ejb-jar.xsd">
    <wls:weblogic-enterprise-bean>
        <wls:ejb-name>IntegrationServiceBeanTAFJ</wls:ejb-name>
        <wls:stateless-session-descriptor>
            <wls:business-interface-jndi-name-map>
                <wls:business-remote>com.temenos.services.integration.ejb.IntegrationServiceBeanRemote</wls:business-remote>
                <wls:jndi-name>ejb/IntegrationServiceBeanRemote</wls:jndi-name>
            </wls:business-interface-jndi-name-map>
        </wls:stateless-session-descriptor>
        <wls:resource-description>
            <wls:res-ref-name>jdbc/t24DataSource</wls:res-ref-name>
            <wls:jndi-name>jdbc/t24DS</wls:jndi-name>
        </wls:resource-description>
        <wls:resource-description>
            <wls:res-ref-name>jdbc/t24LockingDataSource</wls:res-ref-name>
            <wls:jndi-name>jdbc/t24LockingDS</wls:jndi-name>
        </wls:resource-description>
        <wls:jndi-name>ejb/IntegrationServiceBeanRemote</wls:jndi-name>
        <wls:local-jndi-name>ejb/IntegrationServiceBeanLocal</wls:local-jndi-name>
    </wls:weblogic-enterprise-bean>
</wls:weblogic-ejb-jar>

