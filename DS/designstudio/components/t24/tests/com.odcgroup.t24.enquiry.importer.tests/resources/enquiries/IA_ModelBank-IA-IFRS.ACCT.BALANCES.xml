<?xml version="1.0" encoding="UTF-8"?>
<enquiry:Enquiry xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:enquiry="http://www.odcgroup.com/t24/Enquiry"
	xmlns:translationDsl="http://www.odcgroup.com/translation/TranslationDsl" name="IFRS.ACCT.BALANCES" fileName="name:/IA_Config:IFRS.ACCT.BALANCES#"
	showAllBooks="false" startLine="4" endLine="99">
	<fixedSelections field="@ID" operand="NotEquals">
		<values>&quot;&quot;</values>
	</fixedSelections>
<!-- mandatory="true" popupDropDown="false"-->
	<customSelection>
		<selection field="@ID" >
			<label xsi:type="translationDsl:LocalTranslations">
				<translations locale="en" text="@ID" />
			</label>
		</selection>
	</customSelection>
<!-- -->
	<fields name="@ID" length="35" alignment="Left" commaSeparator="false" singleMulti="Single">
		<label xsi:type="translationDsl:LocalTranslations">
			<translations locale="en" text="Transaction Id" />
		</label>
		<position column="1" />
		<operation xsi:type="enquiry:ApplicationFieldNameOperation" field="@ID" />
	</fields>
	<fields name="ACCT.HEAD.TYPE" length="35" alignment="Left" commaSeparator="false" singleMulti="Multi">
		<label xsi:type="translationDsl:LocalTranslations">
			<translations locale="en" text="Accounting Method" />
		</label>
		<position column="2" />
		<operation xsi:type="enquiry:ApplicationFieldNameOperation" field="ACCT.HEAD.TYPE" />
	</fields>
	<fields name="BALANCE" length="35" alignment="Right" commaSeparator="false" singleMulti="Multi">
		<label xsi:type="translationDsl:LocalTranslations">
			<translations locale="en" text="Balance" />
		</label>
		<position column="3" />
		<operation xsi:type="enquiry:ApplicationFieldNameOperation" field="BALANCE" />
	</fields>
	<fields name="LAST.CALC.DATE" length="35" alignment="Right" commaSeparator="false" singleMulti="Multi">
		<label xsi:type="translationDsl:LocalTranslations">
			<translations locale="en" text="Last Calc date" />
		</label>
		<position column="4" />
		<operation xsi:type="enquiry:ApplicationFieldNameOperation" field="LAST.CALC.DATE" />
	</fields>
	<fields name="LAST.CALC.AMT" length="8" alignment="Right" commaSeparator="false" singleMulti="Multi">
		<label xsi:type="translationDsl:LocalTranslations">
			<translations locale="en" text="Last Calc Amt" />
		</label>
		<position column="5" />
		<operation xsi:type="enquiry:ApplicationFieldNameOperation" field="LAST.CALC.AMT" />
	</fields>
</enquiry:Enquiry>