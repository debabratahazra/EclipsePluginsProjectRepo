	EB_CHALLENGE_RESPONSE__VERSION
	{
		/* Defines the application and version */
		FIELD_NAME -> byValue EB_ARC:EB_CHALLENGE_RESPONSE__VERSION__FIELD_NAME *
	}
	EB_CHALLENGE_RESPONSE__VERSION__FIELD_NAME
	{
		/* Indicate the field name to be used for Challenge response method.
This field indicates the starting of the associated Multi set. More than one field can be defined for challenge response method per version.
Restrictions on the data that can be entered into the token device as a challenge:
• Must be numeric (no decimal point)
• Must be at least 4 digits (although we could pad with zeros)
• Must be a maximum of 8 digits */
		FIELD_NAME : T24BusinessTypes:A required
			@t24:i (align=L, inputBehaviour=I, maxLength=50, sysNumber=3.0, core=true, t24Name=FIELD.NAME)
		/* To indicate the start range of the field value. */
		START_RANGE : mml:Integer required
			@t24:i (align=R, inputBehaviour=I, maxLength=5, sysNumber=4.0, core=true, t24Name=START.RANGE)
		/* Indicates the end range of the field value. */
		END_RANGE : mml:Integer required
			@t24:i (align=R, inputBehaviour=I, maxLength=5, sysNumber=5.0, core=true, t24Name=END.RANGE)
		/* The actual Question that is to be shown to the end-user */
		TEXT : T24BusinessTypes:A required
			@t24:i (align=L, inputBehaviour=I, maxLength=50, sysNumber=6.0, core=true, t24Name=TEXT)
	}
