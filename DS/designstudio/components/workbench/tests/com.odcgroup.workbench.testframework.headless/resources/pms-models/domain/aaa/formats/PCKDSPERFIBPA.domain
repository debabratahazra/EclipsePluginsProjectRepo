			@sql:SQLName (value=MARKET_SEGMENT) 
			@aaa:TripleA (AttrMultiLanguage=true, FormatEltName=MARKET_SEGMENT, Rank=120, ScriptDef="{This script allows constructing the market structure according to the benchmark strategy associated to the portfolio for performance analysis: attribution and contribution. It is based on a recurisve algorithm using parent-child relationship between the market segments, from the leaf lower levels up to strategy root NB: it requires defining name and/or denominations for market segments defined in grid} {----------------------------------------------} TRIMSTR(PRINTF({%s[HIERLEV%s]}\"%s[HIERLEV%s%s]%s\", {( IF(DOMAIN().port_cons_rule_e = 1 AND object_id = NULL, {then 2} GET_OBJECT(\"portfolio\",DOMAIN().port_object_id).name, {else 2} GET_OBJECT(\"portfolio\", object_id).name) ), ( IF(DOMAIN().port_cons_rule_e = 1 AND object_id = NULL, {then 2} GET_OBJECT(\"portfolio\",DOMAIN().port_object_id).code, {else 2} GET_OBJECT(\"portfolio\", object_id).code) ),} @PERIOD_DISPLAY, @PORTFOLIO_CODE, @PERIOD_DISPLAY, IF(market_segment_id <> NULL, {Create market structure for tree view according to market segments of the strategy} PRINTF(\"%s%s%s%s%s%s%s%s%s\", {level+8} IF(parent_id.parent_id.parent_id.parent_id.parent_id.parent_id.parent_id.parent_id.market_segment_id <> NULL, PRINTF(\"%s[HIERLEV%i]\", IF(parent_id.parent_id.parent_id.parent_id.parent_id.parent_id.parent_id.parent_id.market_segment_id.DENOMINATION[TSL_CURRENT_LANGUAGE()].denom <> NULL, {Denom in Domain Language or Default if Exist} parent_id.parent_id.parent_id.parent_id.parent_id.parent_id.parent_id.parent_id.market_segment_id.DENOMINATION[DOMAIN().language_dict_id].denom, {No Denom => Name} parent_id.parent_id.parent_id.parent_id.parent_id.parent_id.parent_id.parent_id.market_segment_id.name ), parent_id.parent_id.parent_id.parent_id.parent_id.parent_id.parent_id.parent_id.market_segment_id ) ), {level+7} IF(parent_id.parent_id.parent_id.parent_id.parent_id.parent_id.parent_id.market_segment_id <> NULL, PRINTF(\"%s[HIERLEV%i]\", IF(parent_id.parent_id.parent_id.parent_id.parent_id.parent_id.parent_id.market_segment_id.DENOMINATION[TSL_CURRENT_LANGUAGE()].denom <> NULL, {Denom in Domain Language or Default if Exist} parent_id.parent_id.parent_id.parent_id.parent_id.parent_id.parent_id.market_segment_id.DENOMINATION[TSL_CURRENT_LANGUAGE()].denom, {No Denom => Name} parent_id.parent_id.parent_id.parent_id.parent_id.parent_id.parent_id.market_segment_id.name ), parent_id.parent_id.parent_id.parent_id.parent_id.parent_id.parent_id.market_segment_id ) ), {level+6} IF(parent_id.parent_id.parent_id.parent_id.parent_id.parent_id.market_segment_id <> NULL, PRINTF(\"%s[HIERLEV%i]\", IF(parent_id.parent_id.parent_id.parent_id.parent_id.parent_id.market_segment_id.DENOMINATION[TSL_CURRENT_LANGUAGE()].denom <> NULL, {Denom in Domain Language or Default if Exist} parent_id.parent_id.parent_id.parent_id.parent_id.parent_id.market_segment_id.DENOMINATION[TSL_CURRENT_LANGUAGE()].denom, {No Denom => Name} parent_id.parent_id.parent_id.parent_id.parent_id.parent_id.market_segment_id.name ), parent_id.parent_id.parent_id.parent_id.parent_id.parent_id.market_segment_id ) ), {level+5} IF(parent_id.parent_id.parent_id.parent_id.parent_id.market_segment_id <> NULL, PRINTF(\"%s[HIERLEV%i]\", IF(parent_id.parent_id.parent_id.parent_id.parent_id.market_segment_id.DENOMINATION[TSL_CURRENT_LANGUAGE()].denom <> NULL, {Denom in Domain Language or Default if Exist} parent_id.parent_id.parent_id.parent_id.parent_id.market_segment_id.DENOMINATION[TSL_CURRENT_LANGUAGE()].denom, {No Denom => Name} parent_id.parent_id.parent_id.parent_id.parent_id.market_segment_id.name ), parent_id.parent_id.parent_id.parent_id.parent_id.market_segment_id ) ), {level+4} IF(parent_id.parent_id.parent_id.parent_id.market_segment_id <> NULL, PRINTF(\"%s[HIERLEV%i]\", IF(parent_id.parent_id.parent_id.parent_id.market_segment_id.DENOMINATION[TSL_CURRENT_LANGUAGE()].denom <> NULL, {Denom in Domain Language or Default if Exist} parent_id.par