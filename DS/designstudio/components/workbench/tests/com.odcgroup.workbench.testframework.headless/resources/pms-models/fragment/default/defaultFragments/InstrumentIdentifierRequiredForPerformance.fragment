# UTF-8
metamodelVersion = 8.0.0.20130123

--- Fragment ---
	cardinality = one
	domainEntity = "AAAIdentifierDatasets:InstrumentIdentifier"
{
	--- Box ---
		width = 0
	{
		--- SearchField ---
			domainAttribute = code
			columns = 25
			chars = 20
			required = true
			bean-property = code
			id = code
			widgetGroup = formSearch
			auto-delay = 700
			nb-chars = 1
			outputDesign = "" !
			resource:///Default/autocompleteoutput/Instrument.module
		{
			Event Search (submit)
			{
				method = post
				call-URI = "<pageflow:continuation/>"
				widget-group-ref = form
				param = "flow-action=searchinstrument;flow-change=contextReload"
				only-changed = true
				modal = false
				prevalidate = false
				Snippet Query
				queryAttributes = "marketThirdSearchCode,issuerThirdSearchCode,extDenomination,searchCode,statusE,extSectorC,riskGeoSearchCode,maturityDate,code,underlyingInstrSearchCode,natureE,refCurrSearchCode,extTelekursSynC,extIsinSynC,name,interestRateP"
				queryMappings = code
				queryOutputModule = "" !
				resource:///Default/module/aaa/administration/instrument/search/InstrSearch.module
				Snippet FilterSet
				id = fs_Nl3
				level = 1
				targetDataset = "AAADSInstrument:InstrumentSearchCriteriaAll"
				{
					Snippet Filter
					attribute = searchCode
					editMode = editable
					filterValue = "valueOf(formSearch.instrumentDimInstrument.code)"
					operator = LIKE
					Snippet Filter
					attribute = activeF
					filterValue = Yes
					operator = EQUAL
					Snippet Filter
					attribute = statusE
					editMode = editable
					filterValue = "Active,None"
					operator = IN
				}
				Snippet FilterSet
				id = fs_f2p
				level = 2
				targetDataset = "AAADSInstrument:InstrumentSearchCriteriaAll"
				{
					Snippet Filter
					attribute = natureE
					editMode = editable
					filterValue = "Index,Commodity,FundShare,Rate"
					operator = IN
				}
			}
			Event OnBlur (switchWidgetProperty)
			{
				widget-group-id = btn_execute_in_domaincontext
				property = enabled
				delay = 300
			}
			Event OnFocus (switchWidgetProperty)
			{
				widget-group-id = btn_execute_in_domaincontext
				property = enabled
				delay = 0
			}
			Event OnBlur (switchWidgetProperty)
			{
				widget-group-id = btn_refresh_in_domaincontext
				property = enabled
				delay = 300
			}
			Event OnFocus (switchWidgetProperty)
			{
				widget-group-id = btn_refresh_in_domaincontext
				property = enabled
				delay = 0
			}
		}
	}
}