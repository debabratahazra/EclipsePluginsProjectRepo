# UTF-8
metamodelVersion = 8.0.0.20130123

--- Fragment ---
	cardinality = one
	domainEntity = "AAADSDomain:DomainContext"
{
	--- Box ---
		boxType = vertical
		verticalAlignment = lead
		width = 0
	{
		--- Grid ---
			cssClass = blank
			gridColumnCount = 2
			gridColumnsWidth = "50,50" !
			gridRowCount = 2
		{
			--- GridCell ---
				gridColumnWidth = 50
			{
				--- LabelDomain ---
					domainAttribute = minStatusE !
					for = minStatusE
					cssClass = grey
					horizontalAlignment = "${corporatehalign}"
					width = 0
					required = true
			}
			--- GridCell ---
				gridColumnWidth = 50
			{
				--- ComboBox ---
					domainAttribute = minStatusE !
					horizontalAlignment = "${corporatehalign}"
					required = true
					bean-property = minStatusE
					id = minStatusE
				{
					Event OnChange (submit)
					{
						method = post
						call-URI = "<pageflow:continuation/>"
						widget-group-ref = form
						target = self
						param = "flow-action=contextReload"
						only-changed = true
						modal = false
						prevalidate = false
					}
				}
			}
			--- GridCell ---
				gridColumnWidth = 50
			{
				--- LabelDomain ---
					domainAttribute = maxStatusE !
					for = maxStatusE
					cssClass = grey
					horizontalAlignment = "${corporatehalign}"
					width = 0
					required = true
			}
			--- GridCell ---
				gridColumnWidth = 50
			{
				--- ComboBox ---
					domainAttribute = maxStatusE !
					horizontalAlignment = "${corporatehalign}"
					required = true
					bean-property = maxStatusE
					id = maxStatusE
				{
					Event OnChange (submit)
					{
						method = post
						call-URI = "<pageflow:continuation/>"
						widget-group-ref = form
						target = self
						param = "flow-action=contextReload"
						only-changed = true
						modal = false
						prevalidate = false
					}
				}
			}
		}
	}
}