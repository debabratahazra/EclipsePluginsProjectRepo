# UTF-8
metamodelVersion = 8.0.0.20130123

--- Fragment ---
	cardinality = one
	domainEntity = "AAADSExtOperation:ExtOperationAll"
{
	--- Box ---
	{
		--- Grid ---
			cssClass = blank
			gridColumnCount = 2
			gridColumnsWidth = "50,50" !
			gridRowCount = 1
		{
			--- GridCell ---
				gridColumnWidth = 50
			{
				--- LabelDomain ---
					domainAttribute = quantityN !
					for = quantityN
					cssClass = grey
					horizontalAlignment = "${corporatehalign}"
					required = true
			}
			--- GridCell ---
				gridColumnWidth = 50
			{
				--- TextField ---
					domainAttribute = quantityN !
					columns = 21
					format = decimal.detailed
					horizontalAlignment = "${corporatehalign}"
					type = decimal
					allow = "\'0123456789.,\'"
					chars = 23
					required = true
					bean-property = quantityN
					id = quantityN
			}
		}
	}
}