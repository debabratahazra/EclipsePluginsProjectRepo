# UTF-8
metamodelVersion = 8.0.0.20130123

--- Module ---
	labels en = "Order Proposal", de = "Order Proposal", fr = "Proposition d\'ordre"
	domainEntity = "AAADSDomain:DomainContext"
	bean-define = DSActivityBean.AAADSDomain.DomainContext
	bean-name = DomainContext
	id = ContextFinal_F____
	tid = 20014643905902
{
	--- Box ---
		boxType = vertical
	{
		--- Include ---
			includeSrc = "" !
			resource:///default/domain/ProposalView.fragment
		--- Grid ---
			cssClass = blank
			gridColumnCount = 1
			gridColumnsWidth = 25 !
			gridRowCount = 1
		{
			--- GridCell ---
				gridColumnWidth = 25
			{
				--- Button ---
