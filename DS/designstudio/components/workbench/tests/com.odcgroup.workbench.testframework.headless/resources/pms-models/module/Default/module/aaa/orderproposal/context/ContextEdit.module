					id = btn_SS17A
					tid = 29645707606602
				{
					Event OnClick (submit) S
					{
						method = post
						call-URI = "<pageflow:continuation/>"
						target = self
						flow-action = showIP ud
						modal = false
						only-changed = false
						prevalidate = false
					}
				}
				--- Conditional ---
					indexOfSelectedChild = 1
				{
					--- ConditionalBody ---
						javaCode = "null == <bean:get-property bean=\"DomainContext\" property=\"strategyDimStrategy\"/>"
						technicalName = NoInvestmentProfile
						boxType = vertical
						isWidgetVisible = false
					--- ConditionalBody ---
						javaCode = true
						technicalName = Default
						boxType = vertical
						isWidgetSelected = true
					{
						--- Button ---
							labels en = Rebalance, de = Strategieabgleich, fr = Réaligner toolTips en = "Propose Orders through Automated Rebalancing", de = "Anlagevorschlag mittels autom. Strategieabgleich", fr = "Proposer des ordres grâce rééquilibrage automatique"
							id = btn_qi30h
							tid = 20080484934937
						{
							Event OnClick (submit)
							tid = 3792684326174416
