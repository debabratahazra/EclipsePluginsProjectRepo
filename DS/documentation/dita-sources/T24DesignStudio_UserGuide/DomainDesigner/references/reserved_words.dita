					<stentry>int</stentry>

					<stentry>short</stentry>

					<stentry>try</stentry>
				</strow>

				<strow>
					<stentry>char</stentry>

					<stentry>final</stentry>

					<stentry>interface</stentry>

					<stentry>static</stentry>

					<stentry>void</stentry>
				</strow>

				<strow>
					<stentry>class</stentry>

					<stentry>finally</stentry>

					<stentry>long</stentry>

					<stentry>strictfp</stentry>

					<stentry>volatile</stentry>
