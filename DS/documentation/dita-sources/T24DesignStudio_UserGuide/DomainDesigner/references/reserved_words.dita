					<stentry>while</stentry>
				</strow>

				<strow>
					<stentry>true</stentry>

					<stentry>false</stentry>

					<stentry>null</stentry>

					<stentry> </stentry>

					<stentry> </stentry>
				</strow>
			</simpletable>
		</section>
