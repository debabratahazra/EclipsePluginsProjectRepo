				<indexterm>keywords</indexterm>
			</keywords>
		</metadata>
	</prolog>

	<refbody>
		<section>
			<p>The following list of reserved words cannot be used as
				identifiers:
			</p>
		</section>

		<section>
			<title>Java keywords</title>
			<simpletable>
				<strow>
					<stentry>abstract</stentry>

					<stentry>continue</stentry>

					<stentry>for</stentry>

					<stentry>new</stentry>

					<stentry>switch</stentry>
				</strow>

				<strow>
					<stentry>assert</stentry>

					<stentry>default</stentry>

					<stentry>goto</stentry>

					<stentry>package</stentry>

					<stentry>synchronized</stentry>
				</strow>

				<strow>
					<stentry>boolean</stentry>

					<stentry>do</stentry>

					<stentry>if</stentry>

					<stentry>private</stentry>

					<stentry>this</stentry>
				</strow>

				<strow>
					<stentry>break</stentry>

					<stentry>double</stentry>

					<stentry>implements</stentry>

					<stentry>protected</stentry>

					<stentry>throw</stentry>
				</strow>

				<strow>
					<stentry>byte</stentry>

					<stentry>else</stentry>

					<stentry>import</stentry>

					<stentry>public</stentry>

					<stentry>throws</stentry>
				</strow>

				<strow>
