	<refbody>
		<section>
			<title>Documentation Generation properties</title>
			<simpletable>
				<sthead>
					<stentry>Cartridge name</stentry>
					<stentry>Description</stentry>
				</sthead>
				<strow>
					<stentry>Domain (XLS)</stentry>
					<stentry>Documentation of the domain model.</stentry>
				</strow>
			</simpletable>
		</section>
		<section>
			<title>Transaltion properties</title>
			<simpletable>
				<sthead>
					<stentry>Property</stentry>
					<stentry>Description</stentry>
				</sthead>
				<strow>
					<stentry>Default language</stentry>
					<stentry>
						<xref href="../../MessageEditor/Concept/DefaultLanguage.dita"> refer to translation editor concept </xref>
					</stentry>
				</strow>
				<strow>
					<stentry>Language of interest</stentry>
					<stentry>
