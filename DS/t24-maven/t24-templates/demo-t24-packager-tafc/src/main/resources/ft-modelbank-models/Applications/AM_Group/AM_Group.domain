			@t24:i (align=L, inputBehaviour=I, maxLength=6, sysNumber=17.0, core=true, businessType=A, t24Name=GROUP.TYPE)
		INVESTMENT_PROGRAM -> SCO_PortfolioMaintenance:INVESTMENT_PROGRAM
			@i18n:Label (en="Investment Pgm")
			@i18n:Tooltip (en="Investment Pgm used for Rebalancing")
			@t24:i (align=L, inputBehaviour=I, maxLength=12, sysNumber=18.0, core=true, businessType=A, t24Name=INVESTMENT.PROGRAM)
		BENCHMARK -> AM_Benchmark:AM_BENCHMARK
			@i18n:Label (en=Benchmark)
			@i18n:Tooltip (en=Benchmark)
			@t24:i (align=L, inputBehaviour=I, maxLength=20, sysNumber=19.0, core=true, businessType=S, t24Name=BENCHMARK)
		RESERVED7 : mml:Integer
			@t24:i (align=R, inputBehaviour=E, maxLength=10, sysNumber=20.0, core=true, t24Name=RESERVED7)
		RESERVED6 : mml:Integer
			@t24:i (align=R, inputBehaviour=E, maxLength=10, sysNumber=21.0, core=true, t24Name=RESERVED6)
		RESERVED5 : mml:Integer
			@t24:i (align=R, inputBehaviour=E, maxLength=10, sysNumber=22.0, core=true, t24Name=RESERVED5)
		RESERVED4 : mml:Integer
