			@t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=0.0, core=true, businessType=A, t24Name=EB.LOG.PARAM)
		DESCRIPTION -> byValue EB_Logging:EB_LOGGING_PARAMETER__DESCRIPTION *
