			@i18n:Label (en="Curr No")
			@t24:i (align=R, inputBehaviour=N, maxLength=4, sysNumber=21.0, core=true, t24Name=CURR.NO)
		INPUTTER -> byValue ENT_EntryCreation:AC_CONSOLIDATE_COND__INPUTTER *
		DATE_TIME -> byValue ENT_EntryCreation:AC_CONSOLIDATE_COND__DATE_TIME *
		AUTHORISER : T24BusinessTypes:A
