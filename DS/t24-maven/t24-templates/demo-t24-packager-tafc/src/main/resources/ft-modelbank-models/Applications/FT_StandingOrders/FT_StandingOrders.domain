		GEN_ADVICE : FT_StandingOrders:STO_TYPE__GEN_ADVICE
			@t24:i (align=L, inputBehaviour=I, maxLength=6, sysNumber=6.0, core=true, t24Name=GEN.ADVICE)
		NON_STP : FT_StandingOrders:STO_TYPE__NON_STP
			@i18n:Label (en="1_201408 1408081226 1_201408 GB0010001")
			@t24:i (align=L, inputBehaviour=I, maxLength=3, sysNumber=7.0, core=true, t24Name=NON.STP)
		FT_TXN_TYPE -> byValue FT_StandingOrders:STO_TYPE__FT_TXN_TYPE *
