			@i18n:Label (en="User Routine")
			@t24:i (align=L, inputBehaviour=I, maxLength=3, sysNumber=2.0, core=true, t24Name=USER.ROUTINE)
		/* This field provides a default value for the field DAYS.DELIVERY  in STO for the STO type of STO.This specifies the number of working days to add to the STO frequency date to give the value date of a standing order. */
		DAYS_DELIVERY : mml:Integer
			@i18n:Label (en="Delivery Days")
