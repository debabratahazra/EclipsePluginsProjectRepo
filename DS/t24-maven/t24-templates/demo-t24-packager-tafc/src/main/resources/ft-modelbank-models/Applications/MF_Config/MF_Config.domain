	MF_AMORT_MASTER
		@t24:i (t24Name=MF.AMORT.MASTER, level=I, type=H, allowedFunctions="A C D E H I L P Q R S", localRefAllowed=false)
	{
		/* This the key of the file MF.AMORT.MASTER.
