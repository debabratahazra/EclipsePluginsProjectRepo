			@i18n:Label (en="Bond Share Flag")
			@i18n:Tooltip (en="Whether the secuirty is a Share or a Bond")
			@t24:i (align=R, inputBehaviour=N, maxLength=1, sysNumber=39.0, core=true, t24Name=BOND.SHARE.FLAG)
		/* Source Tax Code used for Source Tax.
