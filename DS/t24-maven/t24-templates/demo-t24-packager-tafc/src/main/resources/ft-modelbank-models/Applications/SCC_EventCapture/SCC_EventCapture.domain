			@t24:i (align=L, inputBehaviour=I, maxLength=16, sysNumber=40.0, core=true, t24Name=SOURCE.TAX.CODE)
		LOCAL_TAX_CODE -> byValue SCC_EventCapture:DIARY__LOCAL_TAX_CODE *
