		UPDATE_IMAGE : WR_Foundation:WR_IMAGE_MANAGEMENT__UPDATE_IMAGE
			@i18n:Label (en="Update Image")
			@i18n:Tooltip (en="Update Image")
			@t24:i (align=L, inputBehaviour=I, maxLength=3, sysNumber=6.0, core=true, t24Name=UPDATE.IMAGE)
