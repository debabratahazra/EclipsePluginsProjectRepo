  <parent>
	<groupId>${project-groupId}</groupId>
	<artifactId>${project-artifactId}</artifactId>
	<version>2015.8.0-SNAPSHOT</version>
    <relativePath>../${project-name}-iris-parent/pom.xml</relativePath>
