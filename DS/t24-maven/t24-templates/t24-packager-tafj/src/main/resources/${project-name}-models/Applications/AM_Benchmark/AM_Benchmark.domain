			@i18n:Label (en=Rate)
			@i18n:Tooltip (en="Rate at which dividend/coupon is being paid")
			@t24:i (align=R, inputBehaviour=I, maxLength=18, sysNumber=12.0, core=true, t24Name=RATE)
		/* NOINPUT field.
Validation Rules */
		DATE_UPDATE : T24BusinessTypes:D
			@i18n:Label (en="Date Update")
			@i18n:Tooltip (en="Date Update")
			@t24:i (align=R, inputBehaviour=I, maxLength=11, sysNumber=13.0, core=true, t24Name=DATE.UPDATE)
		/* Daily update of the market capitalization of the underlying benchmark instrument.
NOINPUT field.Should be interfaced to the source table of the underlying instrument where this information is captured.
Validation Rules */
