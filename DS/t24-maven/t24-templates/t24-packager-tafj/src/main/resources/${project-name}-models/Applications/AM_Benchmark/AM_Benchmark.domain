			@i18n:Label (en="Last Mkt Value")
			@i18n:Tooltip (en="Last Mkt Value")
			@t24:i (align=R, inputBehaviour=I, maxLength=18, sysNumber=14.0, core=true, t24Name=LAST.MKT.VALUE, typeModifiers="4D")
