/* ParameterDefinitions for Collateral */
Domain CO_Config
	@t24:i (module=CO)
	@java:Package (value=com.temenos.t24.datamodel.co.co_config)
namespace "http://www.temenos.com/t24/CO_Config"
metamodelVersion 1.30.6
Classes {
	/* COLLATERAL CODE

The information on this file is used to identify categories of collateral right.
The collateral code file permits definition of the allowable collateral (object) types which may occur under each category of right and permits the definition of a default frequency of review of these rights.
This collateral code is also used in determining how collateral values are reallocated across central bank reporting lines. */
	COLLATERAL_CODE
		@t24:i (t24Name=COLLATERAL.CODE, level=I, type=H, allowedFunctions="A C D E H I L P Q R S", localRefAllowed=false)
	{
		/* Unique identification of collateral code.
The identifier of a category of collateral rights.
Validation Rules
1 to 3-digit numeric. */
		COLLATERAL_CODE -> CO_Config:COLLATERAL_CODE PK required
			@i18n:Label (en="Collateral Code")
			@i18n:Tooltip (en="Indicates the Collateral attached to the Limit")
			@t24:i (align=R, inputBehaviour=I, maxLength=3, sysNumber=0.0, core=true, t24Name=COLLATERAL.CODE)
		DESCRIPTION -> byValue CO_Config:COLLATERAL_CODE__DESCRIPTION *
		SHORT_NAME -> byValue CO_Config:COLLATERAL_CODE__SHORT_NAME *
		COLLATERAL_TYPE -> byValue CO_Config:COLLATERAL_CODE__COLLATERAL_TYPE *
		/* Determines when the system is to perform a recalculation of the percentage of cover applicable to collateral rights held under this code.
A combined date & frequency field which determines if and when the system is to automatically recalculate the value of the PERCENTAGE COVER (field 4) of collateral right main file records belonging to this code.
When such a recalculation is performed, the date part of this field is automatically cycled according to the frequency part.
Validation Rules
17 type FQU (frequency format). (Optional input) */
		PERCENT_DATE_FQU : T24BusinessTypes:FQU
			@t24:i (align=L, inputBehaviour=I, maxLength=17, sysNumber=4.0, core=true, t24Name=PERCENT.DATE.FQU, mask="RDD DDD DDDD #####")
		/* The default review frequency applicable to this category of collateral right.
The content of this field is used to generate a default value for the REVIEW DATE FQU (field 6) when input is made to the collateral right main file using this code.
The permitted values of the frequency code are as follows:
BSNSS - business (working) days; date is to be cycled each business day,
DAILY - date is to be cycled daily (ie. each calendar day),
WEEKn - where n is a number in the range 1 to 9; date is to be cycled every n weeks,
TWMTH - twice monthly; date cycles twice monthly on the 15th and last day of each month,
Mmmdd - where mm is a number in the range 1 to 12 and dd is a number in the range 1 to 31; describes a cycle on a particular day (dd) each month or every mm months.
These frequency codes correspond to the equivalent codes contained in the standard date/frequency combined format.
Validation Rules
5 alphanumeric characters (frequency code). (Optional input) */
		REVIEW_FQU : T24BusinessTypes:A
			@i18n:Label (en="Review Frequency")
			@i18n:Tooltip (en="Review Frequency")
			@t24:i (align=L, inputBehaviour=I, maxLength=5, sysNumber=5.0, core=true, t24Name=REVIEW.FQU)
		RECORD_STATUS : T24BusinessTypes:STAT
			@i18n:Label (en="Record Status")
			@t24:i (align=L, inputBehaviour=N, maxLength=4, sysNumber=6.0, core=true, t24Name=RECORD.STATUS)
		CURR_NO : mml:Integer
			@i18n:Label (en="Curr No")
			@t24:i (align=R, inputBehaviour=N, maxLength=4, sysNumber=7.0, core=true, t24Name=CURR.NO)
		INPUTTER -> byValue CO_Config:COLLATERAL_CODE__INPUTTER *
		DATE_TIME -> byValue CO_Config:COLLATERAL_CODE__DATE_TIME *
		AUTHORISER : T24BusinessTypes:A
			@i18n:Label (en=Authoriser)
			@i18n:Tooltip (en="Indicates the User who Authorises the record")
			@t24:i (align=L, inputBehaviour=N, maxLength=40, sysNumber=10.0, core=true, t24Name=AUTHORISER)
		CO_CODE -> ST_CompanyCreation:COMPANY
			@i18n:Label (en=Company)
			@t24:i (align=L, inputBehaviour=N, maxLength=11, sysNumber=11.0, core=true, businessType=A, t24Name=CO.CODE, mask="R##-###-####")
		DEPT_CODE -> ST_Config:DEPT_ACCT_OFFICER
			@i18n:Label (en="Dept Code")
			@t24:i (align=R, inputBehaviour=N, maxLength=4, sysNumber=12.0, core=true, t24Name=DEPT.CODE)
		AUDITOR_CODE : T24BusinessTypes:A
			@i18n:Label (en="Auditor Code")
			@i18n:Tooltip (en="Auditor Code")
			@t24:i (align=L, inputBehaviour=E, maxLength=16, sysNumber=13.0, core=true, t24Name=AUDITOR.CODE)
		AUDIT_DATE_TIME : T24BusinessTypes:RELTIME
			@i18n:Label (en="Audit Date Time")
			@i18n:Tooltip (en="Date and time when record was audited")
			@t24:i (align=L, inputBehaviour=E, maxLength=15, sysNumber=14.0, core=true, t24Name=AUDIT.DATE.TIME, mask="RDD DD  DD ##:##")
	}
	COLLATERAL_CODE__DESCRIPTION
	{
		/* Description of the category of collateral right.
Validation Rules
Up to 35 characters free text. Multilanguage field. (Mandatory input) */
		DESCRIPTION : T24BusinessTypes:A required
			@t24:i (align=L, inputBehaviour=I, maxLength=35, multiLanguage=true, sysNumber=1.0, core=true, t24Name=DESCRIPTION)
	}
	COLLATERAL_CODE__SHORT_NAME
	{
		/* Short name of the category of collateral right.
Validation Rules
Up to 15 characters free text. Multilanguage field. (Mandatory input) */
		SHORT_NAME : T24BusinessTypes:A required
			@t24:i (align=L, inputBehaviour=I, maxLength=15, multiLanguage=true, sysNumber=2.0, core=true, t24Name=SHORT.NAME)
	}
	COLLATERAL_CODE__COLLATERAL_TYPE
	{
		/* The collateral type or types which are normally associated with this code.
Input to this field must be a valid collateral type defined on the collateral type file.
The collateral code usually relates to a particular type of object, in which case this type may be entered in this field.
When more than one possible type of object occurs under a given type of right, all the allowable types should be mentioned here.
In either case, the first value entered in this field (for the code which has been allocated to the corresponding right) is taken as the default value of the COLLATERAL TYPE (field 1) on the collateral main file. Input of any other type of collateral may be made on the main file, but will require acceptance of an override.

Note:      If no input is made to this field, the system will            not link automatically from the collateral right            application to the collateral (object) application            when this code has been used.
Validation Rules
1 to 3-digit numeric. Multivalue field. (Optional input)
Values may not be duplicated. */
		COLLATERAL_TYPE -> CO_Config:COLLATERAL_TYPE
			@t24:i (align=R, inputBehaviour=I, maxLength=3, sysNumber=3.0, core=true, t24Name=COLLATERAL.TYPE)
	}
	COLLATERAL_CODE__INPUTTER
	{
		INPUTTER : T24BusinessTypes:A
			@t24:i (align=L, inputBehaviour=N, maxLength=40, sysNumber=8.0, core=true, t24Name=INPUTTER)
	}
	COLLATERAL_CODE__DATE_TIME
	{
		DATE_TIME : T24BusinessTypes:RELTIME
			@t24:i (align=L, inputBehaviour=N, maxLength=15, sysNumber=9.0, core=true, t24Name=DATE.TIME, mask="RDD DD  DD ##:##")
	}
	/* This file permits the company-level definition of system parameters. */
	COLLATERAL_PARAMETER
		@t24:i (t24Name=COLLATERAL.PARAMETER, level=I, type=U, allowedFunctions="A C D E I L P Q R S", localRefAllowed=false)
	{
		/* The company for which the parameter record applies.
The collateral parameter table is a company-level table. The item-id to the parameter record must exist as a valid company code on the company file.
In a single-company environment, enter the id (or mnemonic) of the company record.
In a multi-company environment, separate parameter records may be entered for each company at which customer-level details are defined.
Validation Rules
Up to 11 alphanumeric characters (company id). */
		COMPANY_ID -> CO_Config:COLLATERAL_PARAMETER PK required
			@i18n:Label (en=Company)
			@t24:i (align=L, inputBehaviour=I, maxLength=11, sysNumber=0.0, core=true, businessType=COM, t24Name=COMPANY.ID, mask="R##-###-####")
		/* Determines to what extent a history of the revaluation of a collateral record is kept (ie. how many previous values).
A numeric parameter which determines how many values are stored on the collateral main file in REVALUED DATE & REVALUED AMOUNT.
Each time the collateral is revalued, the system records the previous value of the EXECUTION VALUE and the date at which the revaluation took place in the fields mentioned above.
This parameter determines how much of this historical information is kept in the system by limiting the number of multivalues used.
When the maximum number of values defined in this field has been reached, the system automatically discards the oldest each time a new one is added.
Zero is allowed. If no value is given, the value one is assumed.
Validation Rules

1 to 2-digit numeric. (Optional input) */
		REVALUATION_INFO : mml:Integer
			@t24:i (align=R, inputBehaviour=I, maxLength=2, sysNumber=1.0, core=true, t24Name=REVALUATION.INFO)
		/* Identifies which descriptive field from the report lines file is used to enrich the source & target line fields on the collateral allocation table.
The value of this field determines which of the values in the F.RE.STAT.REP.LINE field DESC is used to provide an enrichment to the collateral allocation fields SOURCE LINE & TARGET LINE.
These source & target fields are recorded on the collateral allocation table using the CRF line numbers (ie. those numbers which form the second part of the key to records on F.RE.STAT.REP.LINE).
In order to clarify the meaning of the line numbers on this table, this parameter may be used to instruct the system to display against them one of the available descriptive lines from the report lines file.
The number in this field indicates which value to use.
If no value is entered here, no descriptive enrichment appears.
Validation Rules

'1', '2' or '3'. (Optional input) */
		STAT_REP_LINE_DESC : CO_Config:COLLATERAL_PARAMETER__STAT_REP_LINE_DESC
			@t24:i (align=R, inputBehaviour=I, maxLength=1, sysNumber=2.0, core=true, t24Name=STAT.REP.LINE.DESC)
		DEFAULT_ADDRESS -> byValue CO_Config:COLLATERAL_PARAMETER__DEFAULT_ADDRESS *
		/* Provides the user with a direct link from the input screen of the collateral right to the input of collateral objects belonging to it.
A parameter which determines whether the collateral right application links automatically to the input screen of the collateral object application. There is no automatic link unless a value is entered here.
The value '1ST' indicates normal processing, whereby input & authorization of a new collateral right links directly into the same function on the corresponding object.
The value 'ALL' indicates that the same link is to be provided each time input (& authorization) of a change is made to an existing collateral right.
Validation Rules
'1ST' or 'ALL'. (Optional input) */
		COLLATERAL_LINK : CO_Config:COLLATERAL_PARAMETER__COLLATERAL_LINK
			@i18n:Label (en="Collateral Link")
			@i18n:Tooltip (en="Collateral Link")
			@t24:i (align=L, inputBehaviour=I, maxLength=3, sysNumber=4.0, core=true, t24Name=COLLATERAL.LINK)
		/* The retention period for liquidated collateral records. A period after the expiry date has been reached during which they remain on the 'live' file.
This parameter controls the first step in the cycle of archiving collateral records.
The content of the field specifies a period of time as follows:
nnD#nn days##
nnM#nn months##
nnY#nn years##
In each case, this period of time must elapse before a set of associated collateral records are moved from the live file and placed on the history file.
This elapsed period is taken from the date of the collateral right's EXPIRY DATE.
Any collateral main file (object) records are moved to history together with their associated right. The status of all records moved is set to 'MAT'.
This process is performed as part of the collateral application's end-of-day. The archiving of history records (their complete removal from disk) is handled by the Archiving module.
Validation Rules

2 to 3 alphanumeric characters; nnx where nn is a number         in the range 1 to 99 and x is either 'D', 'M' or 'Y'. (Optional         input) */
		RETENTION_PERIOD : T24BusinessTypes:A
			@i18n:Label (en="Retention Period")
			@t24:i (align=L, inputBehaviour=I, maxLength=4, sysNumber=5.0, core=true, t24Name=RETENTION.PERIOD)
		/* Reserved for future use.
Validation Rules

This is a NOINPUT field. */
		LIMIT_INTERFACE : mml:Integer
			@t24:i (align=R, inputBehaviour=N, maxLength=1, sysNumber=6.0, core=true, t24Name=LIMIT.INTERFACE)
		/* Reserved for future use.
Validation Rules

This is a NOINPUT field. */
		ACCOUNT_BALANCE : CO_Config:COLLATERAL_PARAMETER__ACCOUNT_BALANCE
			@t24:i (align=L, inputBehaviour=I, maxLength=2, sysNumber=7.0, core=true, t24Name=ACCOUNT.BALANCE)
		/* This field controls online update of cash collaterals. If cash collateral such as account, deposits (LD, MM), securities and fiduciary are attached to a limit record; any change in these collateral value are updated during the End of Day process. If such collateral, if value to be updated online then this field is to be marked as "Y". (LIMIT record where the cash collateral are attached, also to be marked "Y" in the online update field along with Online update field marked "Y" in COLLATERAL.TYPE) In such cases the collateral is recalculated when the limit reference is drawn. If the field is marked "N" or left blank then no online update process is done.
Validation Rules


Optional Input field
Input can be Y,N or null
Input allowed only if the execution value(field 5) contains value F (FED)
Online update of cash collateral is done only when online update fields in






COLLATERAL.PARAMETER, COLLATERAL.TYPE & LIMIT records are marked "Y" */
		ONLINE_UPDATE : CO_Config:COLLATERAL_PARAMETER__ONLINE_UPDATE
			@i18n:Label (en="Online Update?")
			@t24:i (align=L, inputBehaviour=I, maxLength=1, sysNumber=8.0, core=true, t24Name=ONLINE.UPDATE)
		COLLATERAL_CCY -> byValue CO_Config:COLLATERAL_PARAMETER__COLLATERAL_CCY *
		/* Determines whether manual allocation of priority in COLLATERAL.RIGHT is allowed.
"NONE" or "MANUAL.
This value must be the same for all companies and so can only be changed via the LIMIT.CHANGE application.
Validation Rules

System-maintained. */
		ORDER_PRIORITY : CO_Config:COLLATERAL_PARAMETER__ORDER_PRIORITY
			@t24:i (align=L, inputBehaviour=I, maxLength=6, sysNumber=11.0, core=true, t24Name=ORDER.PRIORITY)
		/* The portion of a LIMIT which is allowed unsecured by collateral may be allowed before any collateral is allocated or it may be allowed after an amount covered by collateral.
If this field is "Y" then the unsecured amount is allowed without any collateral being needed and only when the unsecured amount is exceeded is collateral allocated. If this field is "N" then an unsecured amount is allowed after no more collateral cover is available.
Blank is treated as "N".
Validation Rules

Y/N or blank */
		COVER_UNSEC_1ST : CO_Config:COLLATERAL_PARAMETER__COVER_UNSEC_1ST
			@i18n:Label (en="Cover Unsec 1st?")
			@i18n:Tooltip (en="Cover Unsecured")
			@t24:i (align=L, inputBehaviour=I, maxLength=1, sysNumber=12.0, core=true, t24Name=COVER.UNSEC.1ST)
		ALLOC_BY_PARAMETER : CO_Config:COLLATERAL_PARAMETER__ALLOC_BY_PARAMETER
			@i18n:Label (en="Allocation by Parameter")
			@t24:i (align=L, inputBehaviour=N, maxLength=3, sysNumber=13.0, core=true, t24Name=ALLOC.BY.PARAMETER)
		REALTIME_ALLOC : T24BusinessTypes:A
			@i18n:Label (en="Real Time Allocation")
			@t24:i (align=L, inputBehaviour=I, maxLength=3, sysNumber=14.0, core=true, t24Name=REALTIME.ALLOC)
		RESERVED10 : mml:Integer
			@t24:i (align=R, inputBehaviour=N, maxLength=55, sysNumber=15.0, core=true, t24Name=RESERVED10)
		RESERVED9 : mml:Integer
			@t24:i (align=R, inputBehaviour=N, maxLength=55, sysNumber=16.0, core=true, t24Name=RESERVED9)
		RESERVED8 : mml:Integer
			@t24:i (align=R, inputBehaviour=N, maxLength=55, sysNumber=17.0, core=true, t24Name=RESERVED8)
		RESERVED7 : mml:Integer
			@t24:i (align=R, inputBehaviour=N, maxLength=55, sysNumber=18.0, core=true, t24Name=RESERVED7)
		RESERVED6 : mml:Integer
			@t24:i (align=R, inputBehaviour=N, maxLength=55, sysNumber=19.0, core=true, t24Name=RESERVED6)
		RESERVED5 : mml:Integer
			@t24:i (align=R, inputBehaviour=N, maxLength=55, sysNumber=20.0, core=true, t24Name=RESERVED5)
		RESERVED4 : mml:Integer
			@t24:i (align=R, inputBehaviour=N, maxLength=55, sysNumber=21.0, core=true, t24Name=RESERVED4)
		RESERVED3 : mml:Integer
			@t24:i (align=R, inputBehaviour=N, maxLength=55, sysNumber=22.0, core=true, t24Name=RESERVED3)
		RESERVED2 : mml:Integer
			@t24:i (align=R, inputBehaviour=N, maxLength=55, sysNumber=23.0, core=true, t24Name=RESERVED2)
		RESERVED1 : mml:Integer
			@t24:i (align=R, inputBehaviour=N, maxLength=55, sysNumber=24.0, core=true, t24Name=RESERVED1)
		RECORD_STATUS : T24BusinessTypes:STAT
			@i18n:Label (en="Record Status")
			@t24:i (align=L, inputBehaviour=N, maxLength=4, sysNumber=25.0, core=true, t24Name=RECORD.STATUS)
		CURR_NO : mml:Integer
			@i18n:Label (en="Curr No")
			@t24:i (align=R, inputBehaviour=N, maxLength=4, sysNumber=26.0, core=true, t24Name=CURR.NO)
		INPUTTER -> byValue CO_Config:COLLATERAL_PARAMETER__INPUTTER *
		DATE_TIME -> byValue CO_Config:COLLATERAL_PARAMETER__DATE_TIME *
		AUTHORISER : T24BusinessTypes:A
			@i18n:Label (en=Authoriser)
