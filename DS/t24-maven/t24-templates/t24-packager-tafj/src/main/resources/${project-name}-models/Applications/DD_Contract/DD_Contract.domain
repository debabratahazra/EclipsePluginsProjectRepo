			@i18n:Tooltip (en="Inward Acct")
			@t24:i (align=L, inputBehaviour=I, maxLength=16, sysNumber=36.0, core=true, businessType=ANT, t24Name=ACCT.FOR.INT.REQ)
		/* This field holds the values as YES when an INWARD mandate is created by the system on CLAIM date while processing an INTERNAL OUTWARD DD.ITEM.
No Input field. Update by System for INTERNAL DD.DDI. */
		INWARD_CREATED : mml:Integer
			@t24:i (align=R, inputBehaviour=N, maxLength=3, sysNumber=37.0, core=true, t24Name=INWARD.CREATED)
		IBAN_DEST_ACCT : T24BusinessTypes:A
			@t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=38.0, core=true, t24Name=IBAN.DEST.ACCT)
