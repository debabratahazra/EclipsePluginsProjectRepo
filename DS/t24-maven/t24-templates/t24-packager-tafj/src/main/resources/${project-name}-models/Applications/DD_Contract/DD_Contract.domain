		REASON_CODE : T24BusinessTypes:A
			@i18n:Label (en="Reason code")
			@t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=18.0, core=true, t24Name=REASON.CODE)
		/* The status related to DD.ITEM which can only be updated by the System. */
		STATUS : T24BusinessTypes:A
			@i18n:Label (en=Status)
			@t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=19.0, core=true, t24Name=STATUS)
		STATUS_HIST -> byValue DD_Contract:DD_ITEM__STATUS_HIST *
		/* Date on which the last claim is made, taken from the mandate (DD.DDI) when processing the last item. */
		MATURE_DATE : T24BusinessTypes:D
			@t24:i (align=R, inputBehaviour=I, maxLength=11, sysNumber=22.0, core=true, t24Name=MATURE.DATE)
		/* The id of the claim fiel that this DD.ITEM has been included in. */
		OUTPUT_REC_ID : T24BusinessTypes:A
			@t24:i (align=L, inputBehaviour=N, maxLength=25, sysNumber=23.0, core=true, t24Name=OUTPUT.REC.ID)
		/* The id of the DD.PARAMETER which was used in the creation process of this record. */
		PARAM_ID -> DD_Contract:DD_PARAMETER
			@i18n:Label (en="Parameter Id")
			@t24:i (align=L, inputBehaviour=N, maxLength=4, sysNumber=24.0, core=true, businessType=A, t24Name=PARAM.ID)
		/* The record id of the DD.REASON.CODES which is used to provide enrichment for the REASON.CODE field since the codes themselves are usually too brief to enable a visual understanding of their meaning. */
		REASON_CODE_ID -> DD_Contract:DD_REASON_CODES
			@t24:i (align=L, inputBehaviour=N, maxLength=35, sysNumber=25.0, core=true, businessType=A, t24Name=REASON.CODE.ID)
		/* This field will have the value as INTERNAL, when a DD.ITEM is created for an internal DD.DDI.
That is, when the REQUEST.TYPE field in DD.DDI is INTERNAL. No Input field. Update by System. */
		REQUEST_TYPE : mml:Integer
			@i18n:Label (en="Request Type")
