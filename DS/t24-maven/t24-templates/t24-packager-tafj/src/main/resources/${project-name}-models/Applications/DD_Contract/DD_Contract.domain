		ACCOUNT_NO -> ACM_AccountOpening:ACCOUNT
			@i18n:Label (en=Account)
			@i18n:Tooltip (en="Indicates Customer Account Number")
			@t24:i (align=L, inputBehaviour=I, maxLength=16, sysNumber=6.0, core=true, businessType=ANT, t24Name=ACCOUNT.NO)
		/* Contains the Currency of the Account. */
		CURRENCY -> FIN_CurrencyConfig:CURRENCY
			@i18n:Label (en=Ccy)
			@t24:i (align=L, inputBehaviour=I, maxLength=3, sysNumber=7.0, core=true, businessType=CCY, t24Name=CURRENCY)
		/* This has the Direct Debit Claim amount and this has to be mapped to outward claim file for the amount to be claimed.
This amount is updated from the STMT.ENTRY local currency amount for which the DD.ITEM is created. */
		AMOUNT : T24BusinessTypes:AMT
			@i18n:Label (en=Amount)
			@i18n:Tooltip (en="Indicates the Amount")
			@t24:i (align=R, inputBehaviour=I, maxLength=19, sysNumber=8.0, core=true, t24Name=AMOUNT)
		/* Their reference as available in the forward STMT.ENTRY is populated here. */
		THEIR_REFERENCE : T24BusinessTypes:A
			@i18n:Label (en="Their Ref")
			@t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=9.0, core=true, t24Name=THEIR.REFERENCE)
		/* Our reference as available in the STMT.ENTRY is updated here. */
		OUR_REFERENCE : T24BusinessTypes:A
			@i18n:Label (en="Transaction Ref")
			@i18n:Tooltip (en="Our Reference")
			@t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=10.0, core=true, t24Name=OUR.REFERENCE)
		/* Account Officer populated in STMT.ENTRY is defaulted here. */
		ACCOUNT_OFFICER -> ST_Config:DEPT_ACCT_OFFICER
			@i18n:Label (en="Account Officer")
			@i18n:Tooltip (en="Indicates Account Officer responsible")
			@t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=11.0, core=true, businessType=A, t24Name=ACCOUNT.OFFICER)
		/* This contains the value date of the claim or the date on which the amount to be claimed. This is updated from the value date as available in STMT.ENTRY.
Number of days as given in Claim.Date.Prd is deducted from this value date to determine the file generation date and file is generated on that day with the file date as this Value date. The claim accounting will be raised on this value date. */
		VALUE_DATE : T24BusinessTypes:D
			@i18n:Label (en="Value Date")
			@i18n:Tooltip (en="Indicates the Value date of the Contract")
			@t24:i (align=R, inputBehaviour=I, maxLength=11, sysNumber=12.0, core=true, t24Name=VALUE.DATE)
