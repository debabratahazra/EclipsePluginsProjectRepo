		RESERVED_13 -> byValue EB_Updates:T24_UPDATE_RELEASE__RESERVED_13 *
		RESERVED_12 -> byValue EB_Updates:T24_UPDATE_RELEASE__RESERVED_12 *
		RELATED_UPDATES -> byValue EB_Updates:T24_UPDATE_RELEASE__RELATED_UPDATES *
		SAVED_VERSION -> byValue EB_Updates:T24_UPDATE_RELEASE__SAVED_VERSION *
		RESTORED_USER -> byValue EB_Updates:T24_UPDATE_RELEASE__RESTORED_USER *
		/* Specifies the workset details.
System maintained field.
Populated from the Key.
Should be a valid entry in EB.UPDATE.RELEASE. */
		WORKSET -> EB_Updates:EB_UPDATE_RELEASE
			@t24:i (align=L, inputBehaviour=N, maxLength=35, sysNumber=28.0, core=true, businessType=ANY, t24Name=WORKSET)
		/* Specifies the product that is present in the c.d attributes.
Mandatory input
Should be a valid product */
		PRODUCT : T24BusinessTypes:A required
			@i18n:Label (en=Product)
			@t24:i (align=L, inputBehaviour=N, maxLength=35, sysNumber=29.0, core=true, t24Name=PRODUCT)
		/* When set to Y.Cd type,c.d number,File name and record name will be populated.
Possible values Y or N. */
		USE_DIMENSIONS : EB_Updates:T24_UPDATE_RELEASE__USE_DIMENSIONS
			@t24:i (align=L, inputBehaviour=N, maxLength=2, sysNumber=30.0, core=true, t24Name=USE.DIMENSIONS)
		SV_FILE_NAME -> byValue EB_Updates:T24_UPDATE_RELEASE__SV_FILE_NAME *
