/* Parameter Definitions for Euro Conversion */
Domain EU_Config
	@t24:i (module=EU)
	@java:Package (value=com.temenos.t24.datamodel.eu.eu_config)
namespace "http://www.temenos.com/t24/EU_Config"
metamodelVersion 1.30.6
Classes {
	EU_CONTRACT_CONVERSION
		@t24:i (t24Name=EU.CONTRACT.CONVERSION, level=F, type=H, allowedFunctions="A C D E H I L P Q R S", localRefAllowed=true)
	{
		/* Represents the reference number to uniquely identify a contract. The contract must already exist.
The format of the key is as follows:
AA YYDDD NNNNN A
where:     AA       -     Application identifier.               YYDDD    -     Year and date of input in julian date format.               NNNNN    -     Sequence number which is unique for the date                              of input.
Validation Rules
10 numeric characters prefixed by 2 characters identifying the application module. */
		TRANS_REF -> EU_Config:EU_CONTRACT_CONVERSION PK required
			@i18n:Label (en="Transaction No")
			@i18n:Tooltip (en="Indicates the transaction reference")
			@t24:i (align=L, inputBehaviour=I, maxLength=17, sysNumber=0.0, core=true, businessType=A, t24Name=TRANS.REF)
		/* This field contains the application to which this contract relates and will normally only contain:
LD.LOANS.AND.DEPOSITS             MG.MORTGAGE             MM.MONEY.MARKET             SWAP
Validation Rules
This is a NOINPUT field. (Populated by the system when the record is created) */
		APPLICATION -> EB_SystemTables:PGM_FILE
			@i18n:Label (en=Application)
			@t24:i (align=L, inputBehaviour=I, maxLength=30, sysNumber=1.0, core=true, businessType=PG, t24Name=APPLICATION, typeModifiers=HULTW)
		/* This field will normally contain an interest payment date.
When conversion takes place, this is the date on which the original contract will be matured (early) and the new contract will begin.
Validation Rules
This is a NOINPUT field.  (Populated by the system when the record is created) */
		CONVERSION_DATE : T24BusinessTypes:D
			@t24:i (align=R, inputBehaviour=I, maxLength=11, sysNumber=2.0, core=true, t24Name=CONVERSION.DATE)
		/* Standard T24 currency field.
Validation Rules
A maximum of 3 characters may be entered.
This is a NOINPUT field.
Must be the key to a valid entry on the CURRENCY file. */
		OLD_CCY -> FIN_CurrencyConfig:CURRENCY
			@t24:i (align=L, inputBehaviour=N, maxLength=3, sysNumber=3.0, core=true, businessType=CCY, t24Name=OLD.CCY)
		/* This field contains the currency to which the contract will be converted and is defaulted from the FIXED.CCY field on the currency record of the original currency.
Validation Rules
This is a NOINPUT field.  (Populated by the system when the record is created) */
		NEW_CCY -> FIN_CurrencyConfig:CURRENCY
			@t24:i (align=L, inputBehaviour=I, maxLength=3, sysNumber=4.0, core=true, businessType=CCY, t24Name=NEW.CCY)
		/* This field contains the limit reference on the original contract.
Validation Rules
This is a NOINPUT field.  (Populated by the system when the record is created) */
		OLD_LIMIT_REF : mml:Integer
			@t24:i (align=R, inputBehaviour=N, maxLength=10, sysNumber=5.0, core=true, t24Name=OLD.LIMIT.REF)
		/* This field contains the new limit reference which will be used as the limit reference for the contract that is matured (early) on the conversion date.
Validation Rules
Standard T24 format for Limit Reference
3 to 7 numeric characters LIMIT.REFERENCE code. (Optional Input; Default value:  see details below).
Input in this field must be a valid LIMIT.REFERENCE number. */
		NEW_LIMIT_REF : mml:Integer
			@t24:i (align=R, inputBehaviour=I, maxLength=10, sysNumber=6.0, core=true, t24Name=NEW.LIMIT.REF)
		/* This field contains the category code to which this deal will be accounted for the transfer of the balance between contracts, i.e. the liquidation account for the old contract and the drawdown for the new contract. An account must exist for this category.
Validation Rules
Standard T24 format for Category.
5 numeric characters Category code        or 2 numeric characters i.e. last 2 digits of valid category code.
Mandatory input.  No Default Value. */
		BAL_SUSP_CAT -> ST_Config:CATEGORY
			@t24:i (align=R, inputBehaviour=I, maxLength=6, sysNumber=7.0, core=true, t24Name=BAL.SUSP.CAT, mask="R##-###")
		/* This field contains the contract ID for the new contract created by the conversion process.
Validation Rules
This is a NOINPUT field.  (Populated by the system when the new contract is created) */
		NEW_CONTRACT : T24BusinessTypes:A
			@t24:i (align=L, inputBehaviour=N, maxLength=14, sysNumber=8.0, core=true, t24Name=NEW.CONTRACT)
		/* This field contains the actual business date on which the conversion took place.
Validation Rules
This is a NOINPUT field. (Populated by the system when the new contract is created) */
		ACTUAL_CONV_DATE : T24BusinessTypes:D
			@t24:i (align=R, inputBehaviour=N, maxLength=11, sysNumber=9.0, core=true, t24Name=ACTUAL.CONV.DATE)
		ORIGINAL_AMOUNT -> byValue EU_Config:EU_CONTRACT_CONVERSION__ORIGINAL_AMOUNT *
		CONVERTED_AMT -> byValue EU_Config:EU_CONTRACT_CONVERSION__CONVERTED_AMT *
		/* Standard T24 alphanumeric field.
Validation Rules
A maximum of 4 characters may be entered.
Must be the key to a valid entry on the FT.TXN.TYPE.CONDITION file. */
		AC_TRANSFER_TYPE -> FT_Config:FT_TXN_TYPE_CONDITION
			@t24:i (align=L, inputBehaviour=I, maxLength=4, sysNumber=12.0, core=true, businessType=A, t24Name=AC.TRANSFER.TYPE)
		/* Standard T24 alphanumeric field.
Validation Rules
A maximum of 4 characters may be entered.
Must be the key to a valid entry on the FT.TXN.TYPE.CONDITION file. */
		OT_TRANSFER_TYPE -> FT_Config:FT_TXN_TYPE_CONDITION
			@t24:i (align=L, inputBehaviour=I, maxLength=4, sysNumber=13.0, core=true, businessType=A, t24Name=OT.TRANSFER.TYPE)
		REPAYMENT_FT -> byValue EU_Config:EU_CONTRACT_CONVERSION__REPAYMENT_FT *
		ERROR_MSG -> byValue EU_Config:EU_CONTRACT_CONVERSION__ERROR_MSG *
		COMMIT_NO : T24BusinessTypes:A
			@t24:i (align=L, inputBehaviour=N, maxLength=25, sysNumber=18.0, core=true, t24Name=COMMIT.NO)
		DRAWDOWN_AMT : T24BusinessTypes:AMT
			@t24:i (align=R, inputBehaviour=N, maxLength=19, sysNumber=19.0, core=true, t24Name=DRAWDOWN.AMT)
		/* If this field is set to 'Y', then it will allow non-interest-payment date conversions, otherwise not. */
		NON_INT_CONV : EU_Config:EU_CONTRACT_CONVERSION__NON_INT_CONV
			@t24:i (align=L, inputBehaviour=I, maxLength=3, sysNumber=20.0, core=true, t24Name=NON.INT.CONV)
		/* Standard T24 alphanumeric field. A maximum of 4 characters may be entered.
Must be the key to a valid entry on the FT.TXN.TYPE.CONDITION file. */
		INT_TRANSFER_TYPE -> FT_Config:FT_TXN_TYPE_CONDITION
			@t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=21.0, core=true, businessType=A, t24Name=INT.TRANSFER.TYPE)
		/* Standard T24 numeric field.A maximum of 5 characters may be entered.
Must be the key to a valid entry on the CATEGORY file. */
		INT_SUSP_CAT -> ST_Config:CATEGORY
			@t24:i (align=L, inputBehaviour=I, maxLength=10, sysNumber=22.0, core=true, businessType=A, t24Name=INT.SUSP.CAT)
		/* Holds the FT id for the interest accrued, once the conversion has taken place. */
		INTEREST_FT : mml:Integer
			@t24:i (align=R, inputBehaviour=N, maxLength=35, sysNumber=23.0, core=true, t24Name=INTEREST.FT)
		/* Holds the interest amount charged, once the conversion has taken place. */
		INTEREST_AMT : mml:Integer
			@i18n:Label (en="Interest Amount")
			@t24:i (align=R, inputBehaviour=N, maxLength=35, sysNumber=24.0, core=true, t24Name=INTEREST.AMT)
		INT_VALUE_DATE : T24BusinessTypes:D
			@t24:i (align=R, inputBehaviour=N, maxLength=35, sysNumber=25.0, core=true, t24Name=INT.VALUE.DATE)
		/* This field is reserved for future expansion.
Validation Rules
This is a NOINPUT field. */
		RESERVED2 : mml:Integer
			@t24:i (align=R, inputBehaviour=N, maxLength=55, sysNumber=26.0, core=true, t24Name=RESERVED2)
		/* This field is reserved for future expansion.
Validation Rules
This is a NOINPUT field. */
		RESERVED1 : mml:Integer
			@t24:i (align=R, inputBehaviour=N, maxLength=55, sysNumber=27.0, core=true, t24Name=RESERVED1)
		OVERRIDE -> byValue EU_Config:EU_CONTRACT_CONVERSION__OVERRIDE *
		RECORD_STATUS : T24BusinessTypes:STAT
			@i18n:Label (en="Record Status")
			@t24:i (align=L, inputBehaviour=N, maxLength=4, sysNumber=29.0, core=true, t24Name=RECORD.STATUS)
		CURR_NO : mml:Integer
			@i18n:Label (en="Curr No")
			@t24:i (align=R, inputBehaviour=N, maxLength=4, sysNumber=30.0, core=true, t24Name=CURR.NO)
		INPUTTER -> byValue EU_Config:EU_CONTRACT_CONVERSION__INPUTTER *
		DATE_TIME -> byValue EU_Config:EU_CONTRACT_CONVERSION__DATE_TIME *
		AUTHORISER : T24BusinessTypes:A
			@i18n:Label (en=Authoriser)
			@i18n:Tooltip (en="Indicates the User who Authorises the record")
			@t24:i (align=L, inputBehaviour=N, maxLength=40, sysNumber=33.0, core=true, t24Name=AUTHORISER)
		CO_CODE -> ST_CompanyCreation:COMPANY
			@i18n:Label (en=Company)
			@t24:i (align=L, inputBehaviour=N, maxLength=11, sysNumber=34.0, core=true, businessType=A, t24Name=CO.CODE, mask="R##-###-####")
		DEPT_CODE -> ST_Config:DEPT_ACCT_OFFICER
			@i18n:Label (en="Dept Code")
			@t24:i (align=R, inputBehaviour=N, maxLength=4, sysNumber=35.0, core=true, t24Name=DEPT.CODE)
		AUDITOR_CODE : T24BusinessTypes:A
			@i18n:Label (en="Auditor Code")
			@i18n:Tooltip (en="Auditor Code")
			@t24:i (align=L, inputBehaviour=E, maxLength=16, sysNumber=36.0, core=true, t24Name=AUDITOR.CODE)
		AUDIT_DATE_TIME : T24BusinessTypes:RELTIME
			@i18n:Label (en="Audit Date Time")
			@i18n:Tooltip (en="Date and time when record was audited")
			@t24:i (align=L, inputBehaviour=E, maxLength=15, sysNumber=37.0, core=true, t24Name=AUDIT.DATE.TIME, mask="RDD DD  DD ##:##")
	}
	EU_CONTRACT_CONVERSION__ORIGINAL_AMOUNT
	{
		/* This field contains the principal for the original contract prior to conversion.
Validation Rules
This is a NOINPUT field.  (Populated by the system when the new contract is created) */
		ORIGINAL_AMOUNT : T24BusinessTypes:AMT
			@t24:i (align=R, inputBehaviour=N, maxLength=19, sysNumber=10.0, core=true, t24Name=ORIGINAL.AMOUNT)
	}
	EU_CONTRACT_CONVERSION__CONVERTED_AMT
	{
		/* This field contains the principal for the new contract after conversion.
Validation Rules
This is a NOINPUT field.  (Populated by the system when the new contract is created) */
		CONVERTED_AMT : T24BusinessTypes:AMT
			@t24:i (align=R, inputBehaviour=N, maxLength=19, sysNumber=11.0, core=true, t24Name=CONVERTED.AMT, typeModifiers="-4")
	}
	EU_CONTRACT_CONVERSION__REPAYMENT_FT
	{
		/* This field contains the ID of the FUNDS.TRANSFER transaction created for contracts where a principal repayment is scheduled for the conversion date. The FUNDS.TRANSFER will debit the original principal liquidation account and credit the suspense account used for early maturity (for loans). This will be vice versa for deposits.
Validation Rules
This is a NOINPUT field.  (Populated by the system when the original contract is matured early) */
		REPAYMENT_FT : T24BusinessTypes:A
			@t24:i (align=L, inputBehaviour=N, maxLength=14, sysNumber=14.0, core=true, t24Name=REPAYMENT.FT)
		REPAYMENT_AMT : T24BusinessTypes:AMT
			@t24:i (align=R, inputBehaviour=N, maxLength=19, sysNumber=15.0, core=true, t24Name=REPAYMENT.AMT, typeModifiers="-4")
	}
	EU_CONTRACT_CONVERSION__ERROR_MSG
	{
		/* This field will contain any errors encountered when trying to either early mature the original contract or create the new contract.
Validation Rules
This is a NOINPUT field.  (Populated by the system whenever errors are encountered) */
		ERROR_MSG : T24BusinessTypes:A
			@t24:i (align=L, inputBehaviour=N, maxLength=50, sysNumber=16.0, core=true, t24Name=ERROR.MSG)
	}
	EU_CONTRACT_CONVERSION__OVERRIDE
	{
		/* Contains details of any overrides applicable to this item.
Validation Rules
This is a NOINPUT field.  (Populated by the system when overrides occur) */
		OVERRIDE : T24BusinessTypes:A
			@t24:i (align=L, inputBehaviour=N, maxLength=55, sysNumber=28.0, core=true, t24Name=OVERRIDE)
	}
	EU_CONTRACT_CONVERSION__INPUTTER
	{
		INPUTTER : T24BusinessTypes:A
			@t24:i (align=L, inputBehaviour=N, maxLength=40, sysNumber=31.0, core=true, t24Name=INPUTTER)
	}
	EU_CONTRACT_CONVERSION__DATE_TIME
	{
		DATE_TIME : T24BusinessTypes:RELTIME
			@t24:i (align=L, inputBehaviour=N, maxLength=15, sysNumber=32.0, core=true, t24Name=DATE.TIME, mask="RDD DD  DD ##:##")
	}
	EU_CONVERSION_AUDIT
		@t24:i (t24Name=EU.CONVERSION.AUDIT, level=I, type=L, allowedFunctions="L P S", localRefAllowed=false)
	{
		/* Standard T24 alphanumeric field.
Validation Rules
A maximum of 20 characters may be entered. */
		AUDIT_KEY -> EU_Config:EU_CONVERSION_AUDIT PK required
			@t24:i (align=L, inputBehaviour=I, maxLength=20, sysNumber=0.0, core=true, businessType=A, t24Name=AUDIT.KEY)
		/* Standard T24 alphanumeric field.
Validation Rules
A maximum of 10 characters may be entered. */
		CONV_TYPE : T24BusinessTypes:A
			@t24:i (align=L, inputBehaviour=I, maxLength=10, sysNumber=1.0, core=true, t24Name=CONV.TYPE)
		/* Validation Rules
A maximum of 35 characters may be entered. */
		CONV_FILE : T24BusinessTypes:ANY
			@t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=2.0, core=true, t24Name=CONV.FILE)
		/* Validation Rules
A maximum of 50 characters may be entered. */
		CONV_REC_ID : T24BusinessTypes:ANY
			@t24:i (align=L, inputBehaviour=I, maxLength=50, sysNumber=3.0, core=true, t24Name=CONV.REC.ID)
		/* Standard T24 alphanumeric field.
Validation Rules
A maximum of 30 characters may be entered. */
		FIELD_NAME : T24BusinessTypes:A
			@i18n:Label (en="Field Name")
			@i18n:Tooltip (en="Name of the field(used mostly for SWIFT)")
			@t24:i (align=L, inputBehaviour=I, maxLength=30, sysNumber=4.0, core=true, t24Name=FIELD.NAME)
		OLD_VALUE -> byValue EU_Config:EU_CONVERSION_AUDIT__OLD_VALUE *
