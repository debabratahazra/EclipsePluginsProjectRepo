			@t24:i (align=R, inputBehaviour=I, maxLength=11, sysNumber=10.0, core=true, t24Name=ONLINE.LIMIT.DATE)
		PRODUCT_ALLOWED -> byValue LI_Config:LIMIT__PRODUCT_ALLOWED *
