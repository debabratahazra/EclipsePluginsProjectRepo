			@i18n:Label (en="Rerun Cancel")
			@i18n:Tooltip (en="Whether to rerun or cancel?")
			@t24:i (align=L, inputBehaviour=I, maxLength=1, sysNumber=15.0, core=true, t24Name=RERUN.CANCEL)
