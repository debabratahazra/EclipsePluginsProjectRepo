			@i18n:Label (en="Risk Class")
			@t24:i (align=L, inputBehaviour=I, maxLength=13, sysNumber=27.0, core=true, t24Name=CALC.RISK.CLASS)
		COMMENTARY_SUMMARY : T24BusinessTypes:A
			@t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=28.0, core=true, t24Name=COMMENTARY.SUMMARY)
		COMMENTARY_DETAIL : T24BusinessTypes:A
			@t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=29.0, core=true, t24Name=COMMENTARY.DETAIL)
		RANKING : mml:Integer
			@t24:i (align=R, inputBehaviour=I, maxLength=2, sysNumber=30.0, core=true, t24Name=RANKING)
		/* No input field reserved for future use.
Validation Rules */
		RESERVED_1 : mml:Integer
			@t24:i (align=R, inputBehaviour=N, maxLength=55, sysNumber=31.0, core=true, t24Name=RESERVED.1)
