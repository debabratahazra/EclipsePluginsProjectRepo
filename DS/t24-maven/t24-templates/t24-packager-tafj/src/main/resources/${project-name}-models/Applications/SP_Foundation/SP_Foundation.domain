		EXECUTED_NOM : mml:Integer
			@t24:i (align=R, inputBehaviour=I, maxLength=18, sysNumber=9.0, core=true, t24Name=EXECUTED.NOM)
		/* This field holds the price to be assigned to PRICE field of SC.EXE.SEC.ORDERS
This forms associated multivalue set along with EXECUTED.NOM and EXE.DELIVERY.REF
Validation Rules

Up to 18 numeric characters */
		EXECUTED_PRICE : mml:Integer
			@t24:i (align=R, inputBehaviour=I, maxLength=18, sysNumber=10.0, core=true, t24Name=EXECUTED.PRICE)
		/* This field holds the MT513 sender's reference to be assigned to BR.EXE.ADV.REF field of SC.EXE.SEC.ORDERS
Validation Rules

Up to 25 Alpha numeric characters */
		EXE_DELIVERY_REF : mml:Integer
			@t24:i (align=R, inputBehaviour=I, maxLength=25, sysNumber=11.0, core=true, t24Name=EXE.DELIVERY.REF)
	}
	/* This file will get  updated  when MT502 is being sent to the broker.
This file is also used to hold details regarding status of inward MT509  and MT513 for all the brokers */
	SP_ORDER_STP_ACTIVITY
		@t24:i (t24Name=SP.ORDER.STP.ACTIVITY, level=F, type=L, allowedFunctions="L P S", localRefAllowed=false)
	{
		/* ID will be same as SEC.OPEN.ORDER ID
Validation Rules
Up to 17 Alpha numeric characters */
		ORDER_ID -> SP_Foundation:SP_ORDER_STP_ACTIVITY PK required
			@i18n:Label (en="Order ID")
			@t24:i (align=L, inputBehaviour=I, maxLength=17, sysNumber=0.0, core=true, businessType=S, t24Name=ORDER.ID)
		BROKER_NO -> byValue SP_Foundation:SP_ORDER_STP_ACTIVITY__BROKER_NO *
