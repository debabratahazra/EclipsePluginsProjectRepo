			@t24:i (align=R, inputBehaviour=I, maxLength=19, sysNumber=15.0, core=true, t24Name=WITHDRAWAL.UNIT)
		DEPOSIT_PERIOD -> byValue ST_ChargeConfig:ACCT_GROUP_CONDITION__DEPOSIT_PERIOD *
		WDL_TXN_CODE -> byValue ST_ChargeConfig:ACCT_GROUP_CONDITION__WDL_TXN_CODE *
		/* This field is reserved for future use
