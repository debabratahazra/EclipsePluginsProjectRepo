	{
		/* This is a multi valued field. Standard T24 numeric field.
Validation Rules
A maximum of 35 characters may be entered.
This is a NOINPUT field. */
		OVERRIDE : mml:Integer
			@t24:i (align=R, inputBehaviour=N, maxLength=35, sysNumber=24.0, core=true, t24Name=OVERRIDE)
	}
	AGENCY__INPUTTER
	{
		INPUTTER : T24BusinessTypes:A
			@t24:i (align=L, inputBehaviour=N, maxLength=40, sysNumber=27.0, core=true, t24Name=INPUTTER)
	}
	AGENCY__DATE_TIME
	{
		DATE_TIME : T24BusinessTypes:RELTIME
			@t24:i (align=L, inputBehaviour=N, maxLength=15, sysNumber=28.0, core=true, t24Name=DATE.TIME, mask="RDD DD  DD ##:##")
	}
	ALT_DEPT_LEVEL
		@t24:i (t24Name=ALT.DEPT.LEVEL, level=I, type=H, allowedFunctions="A C D E H I L P Q S", localRefAllowed=false)
	{
		/* This is the key to the ALT.DEPT.LEVEL records and defines the levels in the alternative departmental hierarchy.
The smaller the level the higher up the hierarchy the level is, e.g. 01 is the highest level.
Validation Rules
Input must be numeric in the range 00 - 99.
(Mandatory input) */
		ALT_LEVEL -> ST_Config:ALT_DEPT_LEVEL PK required
			@t24:i (align=R, inputBehaviour=I, maxLength=2, sysNumber=0.0, core=true, t24Name=ALT.LEVEL)
		/* A description of this level of the hierarchy.
Validation Rules
Up to 30 alphanumeric characters.
