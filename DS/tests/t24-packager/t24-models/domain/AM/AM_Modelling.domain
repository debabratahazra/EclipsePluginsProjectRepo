 AM_POS__TYPE 
 { 
 /* Multi-valued field associated with all the other fields in the file to give details of a portfolio position.Specifies a type of position SIMULATION HEDGING SCENARIO POSITION. It depends on the clients routine defined in the  SET.RTN field of the AM.PARAMETER file.Validation RulesNo input       fieldAutomatically       updated whenever the clients routine set in the SET.RTN field is       executed */ 
 TYPE : mml:string required 
 @t24:i (align=L, inputBehaviour=I, maxLength=10, sysNumber=2.0, t24Name=TYPE) 
 
 /* Multi-valued field associated with all the other fields in the file to give details of a portfolio position.This field will contain the ID of the module that gave rise to this position. For example is the position is a security position then this field will contain 'SC' and if it is a Money Market transaction then this field will contain 'MM' etc. Validation RulesNo input       fieldAutomatically       updated whenever the clients routine set in RTN field is executed */ 
 APPLICATION : mml:string required 
 @t24:i (align=L, inputBehaviour=I, maxLength=2, sysNumber=3.0, t24Name=APPLICATION) 
 
 /* The OPTION field returned from the AM.ENTITY associated with this position.Is set according to SET.POSITION routine. (see SET.POS.RTN field in AM.PARAMETER) */ 
 OPTION : mml:string 
 @t24:i (align=L, inputBehaviour=I, maxLength=5, sysNumber=4.0, t24Name=OPTION) 
 
 /* Multi-valued field associated with all the other fields in the file to give details of a portfolio position.Contains the SECURITY.NO from the SC.POS.ASSET record.Validation RulesNo       input fieldAutomatically       updated whenever the clients routine set in the SET.RTN field is       executed */ 
 CODE : mml:string required 
 @t24:i (align=L, inputBehaviour=I, maxLength=30, sysNumber=5.0, t24Name=CODE) 
 
 /* Multi-valued field associated with all the other fields in the file to give details of a portfolio position.Contains the SECURITY.CURRENCY from the SC.POS.ASSET record.Validation RulesNo       input fieldAutomatically       updated whenever the clients routine set in the SET.RTN field is executed */ 
 CURRENCY : mml:string required 
 @t24:i (align=L, inputBehaviour=I, maxLength=3, sysNumber=6.0, t24Name=CURRENCY) 
 
 /* Multi-valued field associated with all the other fields in the file to give details of a portfolio position.Contains the amount of the ESTIMATION field and the ACCRUED.INT field from the SC.POS.ASSET record.Validation RulesNo       input fieldAutomatically       updated whenever the clients routine set in the SET.RTN field is       executed */ 
 VALUATION : mml:string required 
 @t24:i (align=L, inputBehaviour=I, maxLength=18, sysNumber=7.0, t24Name=VALUATION) 
 
 /* Nominal value of SC position */ 
 NOMINAL : mml:string 
 @t24:i (align=L, inputBehaviour=I, maxLength=15, sysNumber=8.0, t24Name=NOMINAL) 
 
 /* Price of this position that is to be used for valuation purposes. */ 
