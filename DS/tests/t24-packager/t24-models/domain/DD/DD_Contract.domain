 /* The date on which the Item to be rejected gets created can be populated hereValidation RulesUp to 9 date characters (standard Date format) - type D.Optional Field */ 
 CREATE_DATE : mml:string 
 @i18n:Label (en="Value Date") 
 @t24:i (align=R, inputBehaviour=I, maxLength=11, sysNumber=8.0, t24Name=CREATE.DATE) 
 
 /* This field to hold the reason for the item rejection. */ 
 REJECT_REASON : mml:string 
 @i18n:Label (en="Reject Reason") 
