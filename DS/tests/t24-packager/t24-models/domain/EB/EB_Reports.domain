# UTF-8
 /* Enquiry Static and Run Time Routines */ 
 Domain EB_Reports 
 @t24:i (module=EB) 
 @java:Package (value=com.temenos.t24.datamodel.eb.eb_reports) 
 namespace "http://www.temenos.com/t24/EB_Reports" 
 metamodelVersion 4.3.0.20110527 
 Classes { 
 /* This application is run, in Verify mode, to spool all the output from an end of day run. The output is sorted by Departement, User (who requested the output), and by report name. The output from the last five end of day runs is maintained by the Report Control System, hence it is possible to re-spool the complete batch output from a previous end of day run. This is maintained regardless of the SPF Hold Batch Output flag. It is possible to allow the immediate spool of batch output, in which case it will be printed in run order, and still re-spool the output in department, user, name sorted order. Two options are allowed, to direct all the output to a specific printer and to supress the printing of 'unrequested reports'. A 'dispatch list' is spooled at the start of the run detailing the department, user and the requested reports. */ 
 SPOOL_BATCH_OUTPUT 
 @t24:i (level=I, type=W, localRefAllowed=false) 
 { 
 /* This is the sequence number by which to identify the batch run. Numeric in the range 1 - 5 where 1 is the latest batch run. This file is automatically maintained by the batch control system. The output from the last five end of day runs only is maintained, with batch log number 1 indicating the latest batch, batch log 2 the previous etc. Validation RulesNumeric, range 1 - 5. (Mandatory Input) */ 
 BATCH_LOG : mml:string PK required 
 @t24:i (align=L, inputBehaviour=I, maxLength=11, sysNumber=0.0, t24Name=BATCH.LOG) 
 
 /* The computer date when the end of day run was initiated. This field is automatically updated by the Batch Control system. Validation RulesDDMMYYYY - computer date. (No-Input field) */ 
 BATCH_START_DATE : mml:string 
 @t24:i (align=R, inputBehaviour=E, maxLength=11, sysNumber=1.0, t24Name=BATCH.START.DATE) 
 
 /* The computer time when the end of day run was initiated. This field is automatically updated by the Batch Control system. Validation RulesHHMM - computer time. (No-Input field) */ 
 BATCH_START_TIME : mml:string 
 @t24:i (align=R, inputBehaviour=E, maxLength=5, sysNumber=2.0, t24Name=BATCH.START.TIME) 
 
 /* The name of the printer to which the batch output should be directed. Must be a valid printer defined on the PRINTER.ID file. If no printer is specified then the SYSTEM printer (defined on PRINTER.ID) is used. Note: If  specific printer is defined for an individual form type (on DE.FORM.TYPE) then this will be used for all reports defined using that form (see REPORT.CONTROL) - regardless of the contents of this field. Validation Rules30 type A (alphanumeric) characters. */ 
 DEST_PRINTER -> EB_Reports:PRINTER_ID 
 @t24:i (align=L, inputBehaviour=I, maxLength=30, sysNumber=3.0, t24Name=DEST.PRINTER) 
 
 /* Flag to indicate whether all reports produced in the batch (end of day) run should be spooled or just those requested by certain users. If "Y" is entered, then all the reports produced in the end of day run will be spooled. If "N" is entered then only those reports which have been requested by specific users (defined in the user file field RPT.TO.RECEIVE) will be spooled. Validation RulesY or N (Mandatory Input) */ 
 SPOOL_ALL_OUTPUT : EB_Reports:SPOOL_BATCH_OUTPUT__SPOOL_ALL_OUTPUT required 
 @t24:i (align=L, inputBehaviour=I, maxLength=1, sysNumber=4.0, t24Name=SPOOL.ALL.OUTPUT) 
 
 /* The 'business' date when the end of day run was initiated. This field is automatically updated by the Batch Control system. Validation RulesDDMMYYYY - 'business' date (No-Input field) */ 
 BANK_DATE : mml:string 
 @i18n:Label (en="Bank Date") 
 @t24:i (align=R, inputBehaviour=E, maxLength=11, sysNumber=5.0, t24Name=BANK.DATE) 
 
 /* Company for which the SPOOL.BATCH.OUTPUT should be produced.Validation Rules:1. Should be a valid company. */ 
 COMPANY_CODE -> ST_CompanyCreation:COMPANY 
 @i18n:Label (en=Company) 
 @t24:i (align=L, inputBehaviour=I, maxLength=11, sysNumber=6.0, t24Name=COMPANY.CODE) 
 
 RECORD_STATUS : mml:string 
 @i18n:Label (en="Record Status") 
 @t24:i (align=L, inputBehaviour=N, maxLength=4, sysNumber=7.0, t24Name=RECORD.STATUS) 
 
 CURR_NO : mml:string 
 @i18n:Label (en="Curr No") 
 @t24:i (align=R, inputBehaviour=N, maxLength=4, sysNumber=8.0, t24Name=CURR.NO) 
 
 INPUTTER -> byValue EB_Reports:SPOOL_BATCH_OUTPUT__INPUTTER * 
 
 DATE_TIME -> byValue EB_Reports:SPOOL_BATCH_OUTPUT__DATE_TIME * 
 
 AUTHORISER : mml:string 
 @i18n:Label (en=Authoriser) 
 @i18n:Tooltip (en="Indicates the User who Authorises the record") 
 @t24:i (align=L, inputBehaviour=N, maxLength=40, sysNumber=11.0, t24Name=AUTHORISER) 
 
 CO_CODE -> ST_CompanyCreation:COMPANY 
 @i18n:Label (en=Company) 
 @t24:i (align=L, inputBehaviour=N, maxLength=11, sysNumber=12.0, t24Name=CO.CODE) 
 
 DEPT_CODE -> ST_Config:DEPT_ACCT_OFFICER 
 @i18n:Label (en="Dept Code") 
 @t24:i (align=R, inputBehaviour=N, maxLength=4, sysNumber=13.0, t24Name=DEPT.CODE) 
 
 AUDITOR_CODE : mml:string 
 @i18n:Label (en="Auditor Code") 
