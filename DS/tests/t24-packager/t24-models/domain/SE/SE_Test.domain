 /* The values allowed are ADD and SUBT.Operation that can be performed on the operands. */ 
 OPER_OPERATION : SE_Test:SEAT_AUTO_COMPARE__EXP_NUMBER__OPER_PART_NO__OPER_OPERATION 
 @t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=14.0, t24Name=OPER.OPERATION) 
 } 
